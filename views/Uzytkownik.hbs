<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <div class="container-fluid">
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav me-auto">
        <li class="nav-item">
          <a class="btn btn-outline-primary" aria-current="page" href="/Register-admin">Nowy użytkownik</a>
          <a class="btn btn-outline-primary" aria-current="page" href="/admin">Powrót</a>
        </li>
      </ul>
    </div>
  </div>
</nav>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Imie</th>
            <th>Login</th>
            <th>Hasło</th>
            <th>Umowa</th>
            <th>Email</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
        <tr>
            <td>{{this.Id}}</td>
            <td>{{this.Imie}}</td>
            <td><a href="/Uzytkownik?id={{this.Id}}">{{this.Login}}</a></td>
            <td>{{this.Haslo}}</td>
            <td>{{this.Umowa}}</td>
            <td>{{this.Email}}</td>
            <td>
                <form action="users/{{this.Id}}/delete" method="POST">
                  <button type="submit" class="btn btn-sm btn-outlince-danger">&times;</button>
                </form>
            </td>
            <td>
              <form action="users/{{this.Id}}/TAK" method="POST">
                <button type="submit" class="btn btn-sm btn-outlince-danger">&times;</button>
              </form>
            </td>
            <td>
              <form action="users/{{this.Id}}/NIE" method="POST">
                <button type="submit" class="btn btn-sm btn-outlince-danger">&times;</button>
              </form>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
